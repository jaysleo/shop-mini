.cart_list .cart_item {
  margin: 0 48rpx;
  padding: 40rpx 0;
  border-bottom: 2rpx #eee solid;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.cart_list .cart_item:last-child {
  border-bottom: 0;
}
.cart_list .cart_item .icon_checkbox {
  width: 34rpx;
  height: 34rpx;
  margin-top: 60rpx;
}
.cart_list .cart_item .icon_checkbox image {
  width: 100%;
  height: 100%;
}
.cart_list .cart_item .image_wrapper {
  width: 150rpx;
  height: 150rpx;
  margin: 0 30rpx;
  background: #eee;
}
.cart_list .cart_item .image_wrapper image {
  width: 100%;
  height: 100%;
}
.cart_list .cart_item .item_right {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.cart_list .cart_item .item_right .title {
  font-size: 28rpx;
  color: #141313;
  margin-bottom: 8rpx;
  margin-right: 40rpx;
}
.cart_list .cart_item .item_right .price {
  font-size: 24rpx;
  color: #FF9D00;
}
.cart_list .cart_item .item_right .num_wrap {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 120rpx;
  height: 40rpx;
  background: #F8F8F8;
  margin-top: 30rpx;
  border-radius: 20rpx;
}
.cart_list .cart_item .item_right .num_wrap .minus, .cart_list .cart_item .item_right .num_wrap .plus {
  position: relative;
  max-width: 30rpx;
  min-width: 30rpx;
  height: 40rpx;
  line-height: 40rpx;
  background: #f7f7f7;
  text-align: center;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.cart_list .cart_item .item_right .num_wrap .minus .row, .cart_list .cart_item .item_right .num_wrap .plus .row {
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -3px;
  width: 7px;
  height: 1px;
  background-color: #b1b1b1;
}
.cart_list .cart_item .item_right .num_wrap .minus .col, .cart_list .cart_item .item_right .num_wrap .plus .col {
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -3px;
  width: 1px;
  height: 7px;
  background-color: #b1b1b1;
}
.cart_list .cart_item .item_right .num_wrap .text_wrap {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.cart_list .cart_item .item_right .num_wrap .text_wrap .text {
  font-size: 22rpx;
  color: #B1B1B1;
