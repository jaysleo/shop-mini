page {
  background: transparent;
}
.bgm {
  width: 100%;
  height: 100%;
}
.bgm .loginbg {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}
.bgm .loginbg image {
  width: 100%;
  height: 100%;
}
.bgm .login_bg {
  width: 100%;
  height: 100%;
  text-align: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  padding: 0 16%;
  position: relative;
}
.bgm .login_bg .center {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.bgm .login_bg .center .title {
  margin-top: 50%;
}
.bgm .login_bg .center .title .img {
  width: 50%;
  height: 105.75rpx;
}
.bgm .login_bg .center .register {
  position: absolute;
  top: 130rpx;
  right: 48rpx;
  font-size: 24rpx;
  color: #FFFFFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  text-align: right;
}
.bgm .login_bg .center .register .icon {
  margin-left: 30rpx;
}
.bgm .login_bg .center .input {
  margin-top: 10%;
  width: 100%;
  height: 100rpx;
  border: solid 2rpx #FFFFFF;
  vertical-align: middle;
  border-radius: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  color: #ffffff;
  /*指定垂直居中*/
}
.bgm .login_bg .center .input .placeColor .u-input__input {
  color: #ffffff !important;
}
.bgm .login_bg .center .input .input_left {
  width: 80rpx;
}
.bgm .login_bg .center .input .input_right {
  width: 80rpx;
}
.bgm .login_bg .center .input .input_class {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  color: #ffffff;
}
.bgm .login_bg .center .input .input_class .u-input__input {
  color: #ffffff !important;
}
.bgm .login_bg .center .input.on {
  background: #ffffff;
}
.bgm .login_bg .center .input.on .placeColor .u-input__input {
  color: #707070 !important;
}
.bgm .login_bg .center .center_foot {
  margin-top: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.bgm .login_bg .center .center_foot .right {
  -webkit-box-flex: 0.98;
  -webkit-flex: 0.98;
          flex: 0.98;
  text-align: right;
  font-size: 20rpx;
  color: #FFFFFF;
}
.bgm .login_bg .center .center_foot .right:active {
  width: 90%;
  color: #B1B1B1;
  border-radius: 5px;
  opacity: 0.9;
}
.bgm .login_bg .center .btn {
  margin-top: 25%;
}
.bgm .login_bg .center .btn .loginbtn {
  width: 100%;
  height: 83rpx;
  line-height: 83rpx;
  background: #ff9d00;
  border-radius: 9rpx;
  box-shadow: 0rpx 2rpx 4rpx 0rpx rgba(0, 0, 0, 0.16);
  font-size: 25rpx;
  color: #FFFFFF;
}
.bgm .login_bg .center .btn .weixinbtn {
  margin-top: 10%;
  width: 100%;
  height: 83rpx;
  line-height: 83rpx;
  background: #1bac69;
  border-radius: 9rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100% 100%;
  background-color: #1BAC69;
  color: #FFFFFF;
  font-size: 25rpx;
}
.bgm .login_bg .center .btn .weixinbtn:active {
  opacity: 0.8;
}

