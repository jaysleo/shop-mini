page {
  background-color: #f0f0f0;
}
.head {
  width: 100%;
  height: 100%;
  background-color: #F0F0F0;
}
.head .bg {
  background: #FFFFFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  padding: 30rpx 50rpx;
  text-align: center;
  line-height: 100%;
}
.head .bg .nav {
  margin-right: 20rpx;
}
.head .bg .title {
  font-size: 30rpx;
  font-weight: 550;
  color: #141313;
}
.head .search_input {
  height: 7%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  padding: 20rpx 5%;
}
.head .search_input .search_box {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.head .ShopList {
  margin: 20rpx 5%;
  background: #FFFFFF;
  border-radius: 24rpx;
}
.head .ShopList .list-item {
  padding: 26rpx 0 32rpx 0;
  margin: 0 26rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  border-bottom: 2rpx solid #eeeeee;
}
.head .ShopList .list-item .image-wrapper {
  width: 258rpx;
  height: 152rpx;
  margin-right: 20rpx;
  position: relative;
  overflow: hidden;
}
.head .ShopList .list-item .image-wrapper image {
  width: 100%;
  height: 100%;
}
.head .ShopList .list-item .image-wrapper .tip {
  position: absolute;
  top: 30rpx;
  right: 0;
  background: #ff9d00;
  height: 34rpx;
  line-height: 34rpx;
  font-size: 20rpx;
  color: #fff;
  border-top-left-radius: 34rpx;
  border-bottom-left-radius: 34rpx;
  padding: 0 24rpx;
}
.head .ShopList .list-item .right {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  overflow: hidden;
}
.head .ShopList .list-item .right .shop-title {
  font-size: 24rpx;
  color: #141313;
  font-weight: bold;
}
.head .ShopList .list-item .right .price-box {
  margin-top: 10rpx;
}
.head .ShopList .list-item .right .price-box .price {
  font-size: 28rpx;
  color: #FF9D00;
  font-weight: bold;
}
.head .ShopList .list-item .right .price-box .noprice {
  padding-left: 20rpx;
  font-size: 18rpx;
  color: #B1B1B1;
  text-decoration: line-through;
}
.head .ShopList .list-item .right .btnwrap {
  margin-top: 20rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.head .ShopList .list-item .right .btnwrap .purchase {
  font-size: 20rpx;
  color: #FF0000;
}
.head .ShopList .list-item .right .btnwrap .action-btn {
  padding: 0 30rpx;
  height: 50rpx;
  line-height: 50rpx;
  background: #FF9D00;
  border-radius: 25rpx;
  font-size: 20rpx;
  margin: 0;
  color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.head .ShopList .list-item .right .btnwrap .action-btn .text {
  margin-left: 12rpx;
}
.head .ShopList .list-item:last-child {
  border-bottom: 0;
}

