.addressH5 {
  background: #F8F8F8;
  width: 100%;
  height: 100%;
}
.addressH5 .body {
  background: #FFFFFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  padding: 30rpx 50rpx;
  text-align: center;
  line-height: 100%;
}
.addressH5 .body .nav {
  margin-right: 20rpx;
}
.addressH5 .body .title {
  font-size: 30rpx;
  font-weight: 550;
  color: #141313;
}
.addressH5 .ZT {
  padding: 0 50rpx;
  background: #FFFFFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  text-align: left;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  font-size: 24rpx;
  color: #707070;
}
.addressH5 .ZT .icon {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 50rpx;
}
.addressH5 .ZT .input {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  margin-left: 20rpx;
  color: #141313;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding-top: 36rpx;
  padding-bottom: 34rpx;
  border-bottom: 2rpx solid #F8F8F8;
}
.addressH5 .ZT .input .u-input__input {
  height: 36rpx;
  min-height: 36rpx !important;
  line-height: 36rpx;
}
.addressH5 .ZT .input .size {
  width: 112rpx;
  color: #707070;
}
.addressH5 .ZT .input .city {
  margin-left: 10rpx;
}
.addressH5 .ZT .input .right {
  width: 20rpx;
}
.addressH5 .tip {
  padding: 26rpx 54rpx;
  font-size: 24rpx;
  color: #707070;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.addressH5 .tip .icon {
  margin-right: 24rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.addressH5 .tip .icon.checked {
  width: 24rpx;
  height: 24rpx;
  border: 4rpx #D4D4D4 solid;
  border-radius: 8rpx;
}
.addressH5 .tip .icon.checked image {
  display: none;
}
.addressH5 .btn {
  margin-top: 30%;
  padding: 0 100rpx;
}

